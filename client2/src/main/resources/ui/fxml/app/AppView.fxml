<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="appViewRoot" minHeight="-1.0" minWidth="-1.0" prefHeight="400.0" prefWidth="600.0" snapToPixel="false" stylesheets="@../../../css/style.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <VBox prefHeight="-1.0" prefWidth="-1.0" spacing="5.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
      <children>
        <Pane fx:id="appViewTopPane" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="NEVER">
          <children>
            <MenuButton fx:id="appViewMenuButton" mnemonicParsing="false" text="Logo">
              <items>
                <MenuItem mnemonicParsing="false" text="Dein Profil" />
                <MenuItem mnemonicParsing="false" text="Einstellungen" />
                <MenuItem mnemonicParsing="false" text="Abmelden" />
              </items>
            </MenuButton>
          </children>
        </Pane>
        <TabPane fx:id="appViewMainTabPane" minHeight="-1.0" minWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" rotateGraphic="false" side="LEFT" tabClosingPolicy="ALL_TABS" VBox.vgrow="ALWAYS">
          <tabs>
            <Tab fx:id="appViewMainTab" closable="false" disable="false" text="Main">
              <content>
                <Pane prefHeight="-1.0" prefWidth="-1.0" />
              </content>
            </Tab>
            <Tab closable="true" disable="false" text="Lobby 999">
              <content>
                <Pane prefHeight="-1.0" prefWidth="-1.0" />
              </content>
            </Tab>
          </tabs>
        </TabPane>
        <Pane fx:id="appViewBottomPane" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="NEVER" />
      </children>
      <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
    </VBox>
  </children>
</AnchorPane>
