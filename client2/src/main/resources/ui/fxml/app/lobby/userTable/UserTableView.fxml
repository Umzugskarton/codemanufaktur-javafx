<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="userTableRoot" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <opaqueInsets>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </opaqueInsets>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <TableView fx:id="userListView" onMouseClicked="#handleListViewClick">
         <columns>
            <TableColumn fx:id="username" editable="false" prefWidth="99.0" text="Username" />
            <TableColumn fx:id="color" editable="false" prefWidth="67.0" text="Color" />
            <TableColumn fx:id="ready" editable="false" prefWidth="53.0" text="Ready" />
         </columns></TableView>
   </children>
</VBox>
